generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  email          String?  @unique
  email_verified Boolean?
  name           String?
  nickname       String?
  picture        String?
  sub            String?
  updated_at     String?
}
